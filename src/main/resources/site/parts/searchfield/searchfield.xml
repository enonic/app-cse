<part>
    <display-name>CSE search field</display-name>
    <form>
        <field-set name="options">
          <label>Options</label>
          <items>
              <input name="searchResultPageId" type="ContentSelector">
                  <label>Optional search result page</label>
                  <occurrences minimum="0" maximum="1"/>
                  <config>
                      <relationshipType>system:reference</relationshipType>
                  </config>
              </input>
              <input name="autoComplete" type="checkbox">
                  <label>Auto complete.</label>
                  <help-text>Note: Newly enabled or added autocompletions may take several hours to appear.</help-text>
                  <occurrences minimum="0" maximum="1"/>
              </input>
            </items>
        </field-set>
    </form>
</part>

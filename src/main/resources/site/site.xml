<?xml version="1.0" encoding="UTF-8"?>
<site>
  <form>
      <field-set name="config">
      <label>Config</label>
      <items>
        <input type="TextLine" name="googleApiKey">
          <label>Google API key</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="TextLine" name="googleCustomSearchEngineId">
          <label>Custom search engine ID</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
      </items>
    </field-set>
  </form>
</site>
